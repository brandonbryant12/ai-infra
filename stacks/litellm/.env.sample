# Copy to .env and adjust as needed
LITELLM_PORT=4000
OPENROUTER_API_KEY=your-openrouter-api-key-here
LITELLM_MASTER_KEY=admin-key-12345

# Jinja2 Template Configuration Options
LITELLM_VERBOSE=false
LITELLM_REQUEST_TIMEOUT=600
LITELLM_ENABLE_CACHING=false
LITELLM_ENABLE_FALLBACKS=false

# Redis Configuration (if caching enabled)
REDIS_HOST=localhost
REDIS_PORT=6379

# Langfuse tracing (populate from stacks/langfuse/.env after bootstrap)
LANGFUSE_HOST=[https://langfuse.brandonbryant.io](https://langfuse.brandonbryant.io)
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=
LANGFUSE_DEBUG=true