# Database
POSTGRES_PASSWORD=changeme

# Security - Generate these with: openssl rand -base64 32
NEXTAUTH_SECRET=
LANGFUSE_SALT=

# URLs
LANGFUSE_URL=https://langfuse.brandonbryant.io
LANGFUSE_PORT=3100

# Features
LANGFUSE_ENABLE_EXPERIMENTAL_FEATURES=false

# Authentication
AUTH_DISABLE_USERNAME_PASSWORD=false
AUTH_DISABLE_SIGNUP=false

# Email (optional)
EMAIL_FROM_ADDRESS=
SMTP_CONNECTION_URL=

# S3 Storage (optional - for large payloads)
S3_ENDPOINT=
S3_ACCESS_KEY_ID=
S3_SECRET_ACCESS_KEY=
S3_BUCKET_NAME=
S3_REGION=

# Telemetry
LANGFUSE_TELEMETRY_ENABLED=true