# OpenWebUI Environment Configuration
# Copy to .env and adjust as needed

# Port for OpenWebUI web interface
OPENWEBUI_PORT=3000

# Required secret key for backend authentication
# Generate with: openssl rand -hex 32
WEBUI_SECRET_KEY=super-secret-key-change-this-in-production

# Enable/disable user signups
ENABLE_SIGNUP=false

# LiteLLM master key for API access
LITELLM_MASTER_KEY=sk-1234567890abcdef

WEBUI_URL=https://ai.brandonbryant.io